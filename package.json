{
    "name": "@elizaos-plugins/plugin-kaia-new",
    "version": "0.1.9",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "latest",
        "@goat-sdk/adapter-vercel-ai": "0.2.0",
        "@goat-sdk/core": "0.4.6",
        "@kaiachain/kaia-agent-kit": "0.0.4",
        "@goat-sdk/wallet-viem": "0.2.0",
        "tsup": "8.3.5",
        "viem": "2.22.2"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "biome lint .",
        "lint:fix": "biome check --apply .",
        "format": "biome format .",
        "format:fix": "biome format --write ."
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
    "@types/node": "^20.0.0",
    "typescript": "5.6.3",
    "vitest": "3.0.5"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:plugin:1.0.0",
        "pluginParameters": {
            "KAIA_KAIASCAN_API_KEY": {
                "type": "string",
                "description": "EVM private key"
            }
        }
    }
}